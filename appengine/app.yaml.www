application: holidays-search
version: 1
runtime: php55
api_version: 1
instance_class: B8
basic_scaling:
  max_instances: 11
  idle_timeout: 10m

handlers:
- url: /favicon.ico
  static_files: favicon.ico
  upload: favicon.ico

- url: /
  static_files: index.html
  upload: index.html
  
- url: /holidays.js
  static_files: holidays.js
  upload: holidays.js  
  
- url: /img
  static_dir: img
  
- url: /resources
  static_dir: resources

- url: /maga_zakodowany_sajtmap.xml
  script: sitemap.php

